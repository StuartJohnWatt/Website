as.integer() > 1) stop("date difference error")
pop_data <- wb(indicator = "SP.POP.TOTL", startdate = 2010, enddate = 2020, country = unique(covid$geoId)) %>%
as_tibble() %>%
select(-c(indicatorID, indicator)) %>%
rename(population = value) %>%
