# Check data
if ((country_data %>% group_by(country, iso3c, iso2c) %>% tally %>% filter(n > 1) %>% nrow()) != 0) stop("Non-unique country data")
if ((country_data %>% filter(is.na(long)|is.na(lat)) %>% nrow()) != 0) stop("Missing coordinate data")
